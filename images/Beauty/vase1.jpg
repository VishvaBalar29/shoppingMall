  <script>
    (async () => {
      const orderPlacedCounter = document.getElementById('counter-100');
      const co2ReducedCounter = document.getElementById('counter-co2');
      const oxygenCounter = document.getElementById('counter-oxygen');
      const waterSavedCounter = document.getElementById('counter-water');

      // Load counters from localStorage
      const storedOrderCount = localStorage.getItem('orderCount') || 1000;
      const storedCo2Count = localStorage.getItem('co2Count') || 1000;
      const storedOxygenCount = localStorage.getItem('oxygenCount') || 1000;
      const storedWaterCount = localStorage.getItem('waterCount') || 1000;

      orderPlacedCounter.innerText = storedOrderCount;
      co2ReducedCounter.innerText = `${(storedOrderCount * 4.3).toFixed(2)}'`;
      oxygenCounter.innerText = `${(storedOrderCount * 4.3).toFixed(2)}'`;
      waterSavedCounter.innerText = `${(storedOrderCount * 5550).toFixed(0)}'`;

      function updateCounters() {
        const currentOrderCount = parseInt(orderPlacedCounter.innerText);
        const newOrderCount = currentOrderCount + 1;
        orderPlacedCounter.innerText = newOrderCount;
        localStorage.setItem('orderCount', newOrderCount);
        co2ReducedCounter.innerText = `${(newOrderCount * 4.3).toFixed(2)}*`;
        oxygenCounter.innerText = `${(newOrderCount * 4.3).toFixed(2)}*`;
        waterSavedCounter.innerText = `${(newOrderCount * 5550).toFixed(0)}*`;
        localStorage.setItem('co2Count', newOrderCount);
        localStorage.setItem('oxygenCount', newOrderCount);
        localStorage.setItem('waterCount', newOrderCount);
      }

      setInterval(updateCounters, 5000);
    })();
  </script>
